{
  "name": "t3kit/t3kit",
  "description": "Starter kit for TYPO3 CMS. Tools, extensions, configurations and templates.",
  "type": "project",
  "keywords": ["t3kit", "typo3", "cms", "starter"],
  "homepage": "http://www.t3kit.com",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "t3kit Community",
      "homepage": "https://github.com/t3kit"
    }
  ],
  "support": {
    "issues": "https://github.com/t3kit/t3kit/issues",
    "forum": "https://typo3.slack.com/messages/t3kit",
    "source": "https://github.com/t3kit/t3kit",
    "docs": "https://github.com/t3kit/t3kit/blob/master/README.md"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/theme_t3kit"
    },
    {
      "type": "git",
      "url": "https://github.com/svewap/realurl_404_multilingual"
    },
    {
      "type": "git",
      "url": "https://github.com/pixelant/pxa_newsletter_subscription"
    },
    {
      "type": "git",
      "url": "https://github.com/pixelant/pxa_form_enhancement"
    },
    {
      "type": "git",
      "url": "https://github.com/pixelant/pxa_cookie_bar"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/TYPO3.UrlForwarding"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "t3kit/theme-t3kit": "dev-master"
  },
  "require-dev": {
    "t3kit/db": "dev-master",
    "behat/behat": "~3.3.0",
    "behat/mink-extension": "~2.2",
    "behat/mink-goutte-driver": "~1.2.1",
    "behat/mink-selenium2-driver": "~1.3.1",
    "guzzlehttp/guzzle": "~6.2.0",
    "patrickbroens/url-forwarding": "~1.3.0"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "."
    }
  }
}
