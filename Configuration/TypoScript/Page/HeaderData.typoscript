page.headerData {

  // Fonts
  150 = TEXT
  150.value (
       <!-- Fonts -->
       <link href="https://fonts.googleapis.com/css?family=Montserrat:300,500,600,900&display=swap" rel="stylesheet">
 )

  # Favicons ==> http://realfavicongenerator.net/
  # add imege theme/src/favicon/favicon.svg
  # npm run favicons
  ####################################
  180 = FLUIDTEMPLATE
  180 {
    // file = EXT:t3kit/Resources/Private/Templates/Favicons.html
    file = TEXT
    file.stdWrap {
      data = site:identifier
      // debugFunc = 2
      wrap = EXT: | /Resources/Public/assets/Favicons/Favicons.html
    }
    dataProcessing {
      110 = TYPO3\CMS\Frontend\DataProcessing\SiteProcessor
      110 {
        as = site
      }
    }
  }

  // # CSS
  ####################################
  // 250 = FLUIDTEMPLATE
  // 250 {
  //   // file = EXT:t3kit/Resources/Private/Templates/Favicons.html
  //   file = TEXT
  //   file.stdWrap {
  //     data = site:identifier
  //     wrap = EXT: | /Resources/Public/assets/production/css/maincss.html
  //   }
  //   dataProcessing {
  //     110 = TYPO3\CMS\Frontend\DataProcessing\SiteProcessor
  //     110 {
  //       as = site
  //     }
  //   }
  // }
}


