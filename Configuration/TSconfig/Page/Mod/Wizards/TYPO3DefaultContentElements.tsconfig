# ###########################
# Groups of Content elements
# ===========================
# t3kit CE groups
# add static and dynamic CE groups
mod.wizards.newContentElement.wizardItems {
  static {
    header = LLL:EXT:t3kit/Resources/Private/Language/ContentElements/locallang.xlf:group.static
    before = dynamic
  }
  dynamic {
    header = LLL:EXT:t3kit/Resources/Private/Language/ContentElements/locallang.xlf:group.dynamic
    after = static
  }
}
# ___________________________

