{
  "name": "t3kit",
  "version": "9.0.0",
  "description": "t3kit project. A TYPO3 website starterkit.",
  "license": "GPL-2.0",
  "authors": "t3kit Community",
  "repository": "t3kit/t3kit",
  "scripts": {
    "dev": "npm-run-all -p css-lint css-compile",
    "prod": "npm-run-all css-lint js-lint css-compile css-prefix css-minify",
    "css-compile": "node-sass --output-style=expanded --source-map=true dev/styles/main.scss  ../Resources/Public/CSS/main.css",
    "css-lint": "stylelint \"dev/styles/**/*.scss\"",
    "css-minify": "cleancss --level=1 --source-map --source-map-inline-sources --output ../Resources/Public/CSS/main.css ../Resources/Public/CSS/main.css",
    "css-prefix": "postcss ../Resources/Public/CSS/main.css --use=autoprefixer --map=false --output=../Resources/Public/CSS/main.css",
    "js-lint": "standard \"dev/js/main/**/*.js\"",
    "js-lint-fix": "standard --fix \"dev/js/main/**/*.js\"",
    "watch-css": "nodemon -e scss -x \"npm run css-compile\" --cache --cache-location .cache/.stylelintcache"
  },
  "browserslist": ["last 2 versions"],
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "clean-css-cli": "^4.3.0",
    "glob": "^7.1.3",
    "http-server": "^0.11.1",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "rollup": "^1.10.1",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "standard": "11.0.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "terser": "^3.17.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=0.10.3 <0.12"
  }
}
